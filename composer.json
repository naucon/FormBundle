{
    "name": "naucon/form-bundle",
    "description": "Bundle to integrate naucon form package into the Symfony framework.",
    "keywords": ["form", "forms", "formbundle", "bundle", "collection", "validation", "csrf", "xsrf", "Cross-Site-Request-Forgery", "symfony", "smarty"],
    "homepage": "https://github.com/naucon/FormBundle",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Sanzenbacher"
        }
    ],
    "require": {
        "php": ">=7.3",
        "naucon/form": "^4.0.0",
        "symfony/config": "^3.2 || ^4.0 || ^5.0",
        "symfony/dependency-injection": "^3.2 || ^4.0 || ^5.0",
        "symfony/finder": "^3.2 || ^4.0 || ^5.0",
        "symfony/http-kernel": "^3.2 || ^4.0 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "suggest": {
        "twig/twig": "Is required for using the twig form extension"
    },
    "autoload": {
        "psr-4": {"Naucon\\Bundle\\FormBundle\\": ""}
    }
}
